          "description": "Получить информацию о пользователе по его ID",
          "produces": ["application/json"],
          "responses": {
            "200": {
              "description": "Успешный ответ",
              "schema": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "integer"
                  },
                  "name": {
                    "type": "string"
                  }
                }
              }
            },
            "404": {
              "description": "Пользователь не найден"
            }
          }
        }
      }
    }
  }
  